{
  "name": "ng2-store",
  "version": "1.0.0",
  "description": "An Easy to use Data Store for Angular 2",
  "main": "build/ng2-store.js",
  "scripts": {
    "build": "rm -rf build && mkdir build && tsc || true",
    "postinstall": "npm run build && jspm install",
    "watch": "rm -rf build && mkdir build && tsc -w",
    "start": "node build/examples/server.js"
  },
  "author": "jhades.dev@gmail.com",
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-alpha.48"
  },
  "devDependencies": {
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "jspm": "^0.16.14",
    "lodash": "^3.10.1",
    "typescript": "^1.7.3"
  },
  "jspm": {
    "dependencies": {
      "immutable": "npm:immutable@^3.7.5",
      "redux": "npm:redux@^3.0.4"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "immutable": "npm:immutable@^3.7.5",
      "reflect-metadata": "npm:reflect-metadata@^0.1.2"
    }
  }
